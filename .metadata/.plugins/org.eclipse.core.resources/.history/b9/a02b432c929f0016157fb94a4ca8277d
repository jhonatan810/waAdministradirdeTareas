(function(){
	"use strict";
	
	var app = angular
				.module("tareasResourceMock", ["ngMockE2E"]);
	
	app.run(function($httpBackend){
		
		var tareas = [];
		var tareasUrl = "/api/tareas";
		
		$httpBackend.whenGet(tareasUrl).respond(tareas);
		$httpBackend.whenGet("/paginas/").passThrough();
		
	});
	
})();